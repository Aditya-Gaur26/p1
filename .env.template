# API Keys
YOUTUBE_API_KEY=your_youtube_api_key_here
OPENAI_API_KEY=your_openai_api_key_here  # Optional, for embeddings
HF_TOKEN=your_huggingface_token_here

# Paths
DATA_DIR=./data
MODEL_DIR=./models
VECTORDB_DIR=./vectordb
OUTPUT_DIR=./outputs

# Model Configuration
BASE_MODEL_NAME=Qwen/Qwen2.5-7B-Instruct
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2

# Training Settings
USE_WANDB=false
WANDB_PROJECT=os-networks-finetuning
WANDB_API_KEY=your_wandb_api_key_here

# Inference Settings
MAX_NEW_TOKENS=512
TEMPERATURE=0.7
TOP_P=0.9

# Feature Flags
ENABLE_YOUTUBE_SUGGESTIONS=true
ENABLE_PAPER_SEARCH=true
ENABLE_CONCEPT_MAPPING=true
